@article{mayo2003symbol,
  author = {Mayo, Michael},
  title = {Symbol Grounding and Its Implications for Artificial Intelligence},
  journal = {AI Communications},
  year = {2003},
  doi = {10.3233/AIC-2003-16303},
  note = {
  CORE ARGUMENT: Expands on Harnad's notion of symbol groundedness in three ways: (1) how robots select best categories for continuous sensory data, (2) how to ground abstract concepts, (3) objective criterion for when symbols become sufficiently grounded. Argues this analysis weakens Searle's position - while Searle may refute Strong AI for present digital computers, he cannot refute computationalism in general.

  RELEVANCE: Addresses practical implementation challenges of symbol grounding. The focus on category selection and abstract concepts highlights gap between elementary grounding and full semantic competence. The defense of computationalism against Searle shows how SGP framework is used to salvage computational approaches - this dialectical role may explain why SGP is formulated to make computational solutions possible.

  POSITION: Computational approach addressing implementation details of grounding. Defends computationalism by showing grounded symbols are possible in principle.
  },
  keywords = {symbol-grounding, robotics, implementation, Low}
}

@article{liu2025algorithmic,
  author = {Liu, Zhangchi},
  title = {An Algorithmic Information-Theoretic Perspective on the Symbol Grounding Problem},
  journal = {arXiv preprint},
  year = {2025},
  volume = {arXiv:2510.05153},
  doi = {10.48550/arXiv.2510.05153},
  note = {
  CORE ARGUMENT: Reformulates SGP using Algorithmic Information Theory (AIT). Grounding is modeled as information compression - a purely symbolic system cannot ground most "worlds" (data strings) because they are algorithmically random and incompressible. Any statically grounded system is incomplete because adversarial incompressible worlds can be constructed. The "grounding act" requires new information that cannot be inferred from existing code. Proves using Chaitin's theorem that meaning is an open-ended process of overcoming information-theoretic limitations.

  RELEVANCE: Recent formal framework providing information-theoretic perspective on SGP's limits. The proof that complete grounding is impossible for finite systems offers rigorous version of skeptical position. However, the framework assumes representational/computational model of meaning - alternative meta-semantic theories (use-based, normative) might reject the information-theoretic framing entirely. Demonstrates how mathematical formalization depends on prior semantic commitments.

  POSITION: Information-theoretic analysis proving fundamental limits on symbol grounding. Establishes impossibility results for complete grounding in finite systems.
  },
  keywords = {symbol-grounding, information-theory, formal, Low}
}

@article{leibovich2016symbol,
  author = {Leibovich, Tali and Ansari, Daniel},
  title = {The Symbol-Grounding Problem in Numerical Cognition: A Review of Theory, Evidence, and Outstanding Questions},
  journal = {Canadian Journal of Experimental Psychology},
  year = {2016},
  volume = {70},
  number = {1},
  pages = {12--23},
  doi = {10.1037/cep0000070},
  note = {
  CORE ARGUMENT: Reviews symbol grounding in numerical cognition specifically - how numerical symbols (words, numerals) acquire meaning. Critically evaluates hypothesis that symbols are grounded in Approximate Number System (ANS). Concludes: (1) current evidence does not support ANS-grounding, (2) continuous variables confound numerosity tasks making pure ANS-symbol association impossible to measure, (3) cognitive control mediates any mapping between ANS and symbols.

  RELEVANCE: Domain-specific analysis of symbol grounding in mathematical cognition. Shows that empirical evidence for dominant grounding theory (ANS mapping) is weak, parallel to broader SGP debate where proposed solutions face empirical and conceptual challenges. The role of cognitive control in mediating grounding suggests higher-level capacities are necessary - supporting views (like Bringsjord's) that mere sensorimotor grounding is insufficient.

  POSITION: Critical review of ANS-grounding hypothesis in numerical cognition. Argues cognitive control, not direct ANS mapping, mediates symbolic number understanding.
  },
  keywords = {symbol-grounding, numerical-cognition, ANS, Low}
}

